<config>

  <Systems>

    <!-- MAPS -->

    <System name="logistic">
      <ModellingTime>10000</ModellingTime>
      <Parameters>
        <Step start="0.5" step="0.5" end="2" default="1"/>
        <Param name="r" start="1" step="0.001" end="4" default="4"/>
