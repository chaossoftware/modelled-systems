<Config>

  <ModelingTask>
    <System>logistic</System>
    <Action>signal</Action>
    <Orthogonalization type="mgs" interval="1"/>
    <!-- cgs , mgs , hh -->

    <Output outDir="Output" picWidth="320" picHeight="240" binaryOutput="false"/>
    <!--picWidth = 384 better for heatmaps-->
  </ModelingTask>

  <Routines>
    <Routine name="signal"/>
    <Routine name="bifurcation" paramIndex="0" iterations="1000"/>
    <Routine name="lle"/>
    <Routine name="lle_sync" convergeRatio="0.0001" iterations="5000"/>
    <Routine name="lle_by_param" paramIndex="0" iterations="400"/>
    <Routine name="le_spec"/>
    <Routine name="le_spec_map" param1Index="0" param2Index="1" iterations="400"/>
    <Routine name="lyapunov_fractal" paramIndex="0" sequence="AB" iterations="400"/>
  </Routines>

  <Systems>

    <!-- Maps -->

    <System name="logistic">
      <Solver name="discrete" time="25000" dt="1"/>
      <Parameters>
        <Param name="r" value="4" range="1..4"/>
      </Parameters>
    </System>

    <System name="henon">
      <Solver name="discrete" time="25000" dt="1"/>
      <Parameters>
        <Param name="a" value="1.4" range="-0.75..2"/>
        <Param name="b" value="0.3" range="-1.15..1.15"/>
      </Parameters>
    </System>

    <System name="tinkerbell">
      <Solver name="discrete" time="25000" dt="1"/>
      <Parameters>
        <Param name="a" value="0.9" range="-1.8..0.905"/>
        <Param name="b" value="-0.6013" range="-0.6..0.27"/>
        <Param name="c" value="2.0" range="-0.201..2.009"/>
        <Param name="d" value="0.5" range="-1.631..0.503"/>
      </Parameters>
    </System>

    <System name="henon_generalized">
      <Solver name="discrete" time="25000" dt="1"/>
      <Parameters>
        <Param name="a" value="1.9" range="-0.5..2"/>
        <Param name="b" value="0.03" range="-1.5..1.5"/>
      </Parameters>
    </System>


    <!-- ODE Systems -->

    <System name="lorenz">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters>
        <Param name="ϛ" value="10.0" range="0..40"/>
        <Param name="r" value="28.0" range="0..150"/>
        <Param name="b" value="8/3" range="0..4"/>
      </Parameters>
    </System>

    <System name="rossler">
      <Solver name="rk4" time="2500" dt="0.1"/>
      <Parameters>
        <Param name="a" value="0.2" range="0..0.5"/>
        <Param name="b" value="0.2" range="0..5"/>
        <Param name="c" value="5.7" range="0..16"/>
      </Parameters>
    </System>

    <System name="thomas">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters>
        <Param name="b" value="0.19" range="0..1"/>
      </Parameters>
    </System>

    <System name="halvorsen">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters>
        <Param name="a" value="1.4" range="0..4"/>
      </Parameters>
    </System>

    <System name="qi_chen">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters>
        <Param name="α" value="38" range="10..56"/>
        <Param name="β" value="8/3" range="0..5"/>
        <Param name="ς" value="80" range="40..250"/>
      </Parameters>
    </System>

    <System name="chua">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters>
        <Param name="α" value="15.6" range="4..40"/>
        <Param name="β" value="1" range="0..5"/>
        <Param name="ς" value="28" range="10..56"/>
        <Param name="δ" value="-1.143" range="-3..3"/>
        <Param name="ε" value="-0.714" range="-3..3"/>
      </Parameters>
    </System>

    <System name="stankevich">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters>
        <Param name="α" value="9" range="NaN..NaN"/>
        <Param name="β" value="0.016" range="NaN..NaN"/>
        <Param name="μ" value="4" range="NaN..NaN"/>
        <Param name="ǫ" value="0.02" range="NaN..NaN"/>
        <Param name="ω₀" value="2*pi" range="NaN..NaN"/>
      </Parameters>
    </System>

    <System name="charo">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters>
        <Param name="A" value="0.1" range="-2.5..2.5"/>
        <Param name="η" value="0.1" range="-2.5..2.5"/>
        <Param name="ω" value="pi/5" range="-2.5..2.5"/>
      </Parameters>
    </System>

    <System name="henon_heiles">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters/>
    </System>

    <System name="anischenko_nikolaev">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters>
        <Param name="α" value="0.2" range="0..0.5"/>
        <Param name="β" value="0.43" range="0..2.5"/>
        <Param name="δ" value="0.001" range="0..2.5"/>
        <Param name="μ" value="0.0809" range="0..2.5"/>
      </Parameters>
    </System>

    <System name="klein_baier">
      <Solver name="rk4" time="250" dt="0.01"/>
      <Parameters>
        <Param name="a" value="0.15" range="NaN..NaN"/>
        <Param name="b" value="0.25" range="NaN..NaN"/>
        <Param name="c" value="0.1" range="NaN..NaN"/>
        <Param name="d" value="0.3922" range="NaN..NaN"/>
        <Param name="e" value="0.05" range="NaN..NaN"/>
      </Parameters>
    </System>

  </Systems>

</Config>